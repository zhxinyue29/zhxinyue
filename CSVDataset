  (embed_tokens): Embedding(151936, 1536)
  (input_projection): Sequential(
    (0): Linear(in_features=1, out_features=1536, bias=True)
    (1): GELU(approximate='none')
    (2): LayerNorm((1536,), eps=1e-05, elementwise_affine=True)
  )
  (rotary_emb): RotaryEmbedding()
  (layers): ModuleList(
    (0-29): 30 x DeepseekBlock(
      (input_norm): DeepseekRMSNorm()
      (attn_q): Linear(in_features=1536, out_features=1536, bias=False)
      (attn_k): Linear(in_features=1536, out_features=256, bias=False)
      (attn_v): Linear(in_features=1536, out_features=256, bias=False)
      (attn_output): Linear(in_features=1536, out_features=1536, bias=False)
      (mlp): Sequential(
        (0): Linear(in_features=1536, out_features=8960, bias=True)
        (1): GELU(approximate='none')
        (2): Linear(in_features=8960, out_features=1536, bias=True)
        (3): Dropout(p=0.1, inplace=False)
      )
      (mlp_norm): DeepseekRMSNorm()
      (attn_dropout): Dropout(p=0.1, inplace=False)
    )
  )
  (final_norm): DeepseekRMSNorm()
  (output): Sequential(
    (0): Linear(in_features=1536, out_features=1536, bias=True)
    (1): GELU(approximate='none')
    (2): Dropout(p=0.1, inplace=False)
    (3): Linear(in_features=1536, out_features=1, bias=True)
  )
)
