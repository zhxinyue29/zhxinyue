    (0-29): 30 x DeepseekBlock(
      (input_norm): DeepseekRMSNorm()
      (attn): DeepseekAttention(
        (q_proj): Linear(in_features=1536, out_features=1536, bias=False)
        (k_proj): Linear(in_features=1536, out_features=256, bias=False)
        (v_proj): Linear(in_features=1536, out_features=256, bias=False)
        (o_proj): Linear(in_features=1536, out_features=1536, bias=False)
      )
      (post_attn_norm): DeepseekRMSNorm()
      (mlp): DeepseekMLP(
        (gate_proj): Linear(in_features=1536, out_features=8960, bias=False)
        (up_proj): Linear(in_features=1536, out_features=8960, bias=False)
        (down_proj): Linear(in_features=8960, out_features=1536, bias=False)
      )
    )
  )
  (final_norm): DeepseekRMSNorm()
  (output): Sequential(
    (0): Linear(in_features=1536, out_features=1536, bias=True)
    (1): GELU(approximate='none')
    (2): Dropout(p=0.1, inplace=False)
    (3): Linear(in_features=1536, out_features=1, bias=True)
  )
